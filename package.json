{
  "name": "@gondi/sdk",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node esbuild.js && tsc --emitDeclarationOnly  --declarationDir dist && tsc-alias",
    "build-check": "run tsc --noEmit",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "fmt": "prettier --config ../../.prettierrc --ignore-path ../../.prettierignore --write .",
    "run-test": "yarn build && tsx --async-stack-traces",
    "gql:types": "graphql-codegen"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/typescript": "^3.0.4",
    "@graphql-codegen/typescript-apollo-client-helpers": "^2.2.6",
    "@graphql-codegen/typescript-generic-sdk": "^3.1.0",
    "@graphql-codegen/typescript-operations": "^3.0.4",
    "@types/node": "^20.4.6",
    "esbuild": "^0.17.19",
    "eslint": "^8.34.0",
    "graphql-codegen": "^0.4.0",
    "jest": "^29.6.2",
    "prettier": "2.6.2",
    "ts-jest": "^29.0.0",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.4.0",
    "tsx": "^3.12.7",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@apollo/client": "^3.7.16",
    "@babel/core": "^7.0.0-0",
    "@types/crypto-js": "^4.1.1",
    "abitype": "^0.8.7",
    "apollo-link-scalars": "^4.0.2",
    "crypto-js": "^4.1.1",
    "ethers": "^6.0.8",
    "graphql": "^16.7.1",
    "graphql-tag": "^2.12.6",
    "siwe": "^2.1.4",
    "viem": "^1.0.7"
  }
}