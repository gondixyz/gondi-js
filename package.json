{
  "name": "gondi",
  "version": "0.3.11",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "docs": "typedoc --options ./typedocconfig.ts --out docs/$npm_package_version && rm -rf docs/lts && cp -r docs/$npm_package_version/ docs/lts/",
    "build": "node esbuild.js && tsc && tsc-alias",
    "build-unbundle": "tsc --outDir dist && tsc-alias",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "gql:types": "graphql-code-generator",
    "prepare": "husky install",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gondixyz/gondi-js.git"
  },
  "homepage": "https://gondixyz.github.io/gondi-js",
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/typescript": "^3.0.4",
    "@graphql-codegen/typescript-apollo-client-helpers": "^2.2.6",
    "@graphql-codegen/typescript-generic-sdk": "^3.1.0",
    "@graphql-codegen/typescript-operations": "^3.0.4",
    "@types/node": "^20.4.6",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "abitype": "^0.8.7",
    "esbuild": "^0.17.19",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "graphql-codegen": "^0.4.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "pinst": "^3.0.0",
    "prettier": "2.6.2",
    "ts-jest": "^29.0.0",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.4.0",
    "tsx": "^3.12.7",
    "typedoc": "^0.24.8",
    "typedoc-plugin-missing-exports": "^2.0.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@apollo/client": "^3.7.16",
    "@reservoir0x/reservoir-sdk": "^1.5.1",
    "apollo-link-scalars": "^4.0.2",
    "ethers": "^6.0.8",
    "graphql": "^16.7.1",
    "graphql-tag": "^2.12.6",
    "siwe": "^2.1.4",
    "viem": "^1.13.1"
  }
}
